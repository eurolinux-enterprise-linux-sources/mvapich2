VPATH = .:@srcdir@
INCLUDES = -I../../../src/include -I${top_srcdir}/src/include -I../../include \
	 -I${srcdir}/../../include

.SUFFIXES:
.SUFFIXES: .o .cxx .txt 
.cxx.o:
	@if [ "x$(VERBOSE)" != "x1" -a "x$(V)" != "x1" ] ; then \
	  echo "  CXX             $<" ; \
	else \
	  echo $(CXX_COMPILE) -c $< ; \
	fi
	@$(CXX_COMPILE) -c $<
.cxx.txt:
	$(CXXCPP) $(INCLUDES) $(CPPFLAGS) $< >$*.txt
.cxx:
	$(CXX_COMPILE) -o $* $< $(LDFLAGS) $(LIBS)

