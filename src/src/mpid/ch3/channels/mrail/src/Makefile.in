	test "x@use_internal_hwloc@" = "xno" ||\
            { ar crv @master_top_builddir@/lib/lib${MPILIBNAME}.a hwloc/src/*.o && ar crv @master_top_builddir@/lib/lib${MPILIBNAME}.la hwloc/src/.libs/*.o; }

