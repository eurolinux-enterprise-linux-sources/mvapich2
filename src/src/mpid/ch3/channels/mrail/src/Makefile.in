# --------------------------------------------------------------------------
# --------------------------------------------------------------------------
# Dependencies
dependencies:
	for dir in rdma @other_subdirs@ - ; do \
		if [ "$$dir" = "-" ] ; then break ; fi ; \
		if [ "$$dir" = "hwloc" ] ; then continue ; fi ;\
		(cd $$dir && ${MAKE} dependencies ) ; done
# End of Dependencies
