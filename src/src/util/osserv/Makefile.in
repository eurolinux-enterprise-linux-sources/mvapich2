# Dependencies
DEPS_DIR = .deps
MAKE_DEPEND_C   = @MAKE_DEPEND_C@ $(DEFS) $(INCLUDES) $(CPPFLAGS)

dependencies: $(DEPS_DIR)/timestamp
$(DEPS_DIR)/dl.d: $(srcdir)/dl.c
	@if [ "x$(VERBOSE)" != "x1" -a "x$(V)" != "x1" ] ; then \
	  echo "MAKE_DEPEND_C dl.c > $(DEPS_DIR)/dl.d" ; \
	else \
	  echo $(MAKE_DEPEND_C)  $(srcdir)/dl.c >$(DEPS_DIR)/dl.d ; \
	fi
	@$(MAKE_DEPEND_C)  $(srcdir)/dl.c >$(DEPS_DIR)/dl.d
$(DEPS_DIR)/timestamp: dl.c Makefile
	rm -f $(DEPS_DIR)/newalldeps
	@if [ "x$(VERBOSE)" != "x1" -a "x$(V)" != "x1" ] ; then \
	  echo "MAKE_DEPEND_C dl.c > $(DEPS_DIR)/dl.d" ; \
	else \
	  echo $(MAKE_DEPEND_C)  $(srcdir)/dl.c >$(DEPS_DIR)/dl.d ; \
	fi
	@$(MAKE_DEPEND_C)  $(srcdir)/dl.c >$(DEPS_DIR)/dl.d
	echo "include $(DEPS_DIR)/dl.d" >>$(DEPS_DIR)/newalldeps
	if [ -s $(DEPS_DIR)/newalldeps ] ; then mv -f $(DEPS_DIR)/newalldeps $(DEPS_DIR)/alldeps ; fi
	date >$(DEPS_DIR)/timestamp
include $(DEPS_DIR)/alldeps
