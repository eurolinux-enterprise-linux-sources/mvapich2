all-postamble:
	build_prefix=`cd ../.. && pwd` && \
	for dir in mpiname - ; do \
            if [ "$$dir" = "-" ] ; then break ; fi ; \
            cd $$dir && ${MAKE} prefix=$$build_prefix exec_prefix=$$build_prefix bindir=$$build_prefix/bin mvapich-build-install; \
        done

distclean-local:
	build_prefix=`cd ../.. && pwd` && \
	for dir in mpiname - ; do \
            if [ "$$dir" = "-" ] ; then break ; fi ; \
            cd $$dir && ${MAKE} prefix=$$build_prefix exec_prefix=$$build_prefix bindir=$$build_prefix/bin mvapich-build-uninstall; \
        done
